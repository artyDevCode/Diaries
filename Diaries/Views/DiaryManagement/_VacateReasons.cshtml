@model  IEnumerable<Diaries.Models.V_Diary_List_VacateReasons>

<head>
    <title>List Vacate Reasons Attributes PartialView</title>
</head>

<table border="0" cellspacing="0" cellpadding="10">
    <tr>
        <td>
            <table border="0" style="font-family: Arial; float: right; font-size: 9pt; background-color: transparent; width: auto; height: 100%; ">
                <tr>
                    <td class="auto-style12">

                        <table border="0" style="font-family: Arial; font-size: 9pt; width: 100%; background-color: transparent;" cellspacing="0" cellpadding="0">
                            @*<tr>
                                    <td style="padding:10px;">

                                        <input type="button" id="AddListAttr" value="Save" class="btn btn-success" />
                                        <input type="button" id="CancelListAttr" value="Cancel" class="btn btn-danger" />

                                    </td>
                                </tr>*@
                            <tr>
                                <td style="padding:10px;">

                                    @if (Model != null)
                                    {
                                        int i = 0;
                                        foreach (var item in Model)
                                        {
                                            if (i == 0)
                                            {
                                                if (@item.V_VR_SelectFilter == "All")
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_AllFilter value=@item.V_VR_AllFilter checked="checked" />@item.V_VR_AllFilter
                                                }
                                                else
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_AllFilter value=@item.V_VR_AllFilter />@item.V_VR_AllFilter
                                                }

                                                if (@item.V_VR_SelectFilter == "Civil")
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_CivilFilter value=@item.V_VR_CivilFilter checked="checked" />@item.V_VR_CivilFilter
                                                }
                                                else
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_CivilFilter value=@item.V_VR_CivilFilter />@item.V_VR_CivilFilter
                                                }

                                                if (@item.V_VR_SelectFilter == "Criminal")
                                                {<input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_CriminalFilter value=@item.V_VR_CriminalFilter checked="checked" />@item.V_VR_CriminalFilter}
                                                else
                                                {<input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_CriminalFilter value=@item.V_VR_CriminalFilter />@item.V_VR_CriminalFilter}

                                                if (@item.V_VR_SelectFilter == "Committals")
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_CommittalsFilter value=@item.V_VR_CommittalsFilter checked="checked" />@item.V_VR_CommittalsFilter
                                                }
                                                else
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_CommittalsFilter value=@item.V_VR_CommittalsFilter />@item.V_VR_CommittalsFilter
                                                }

                                                if (@item.V_VR_SelectFilter == "Intervention Orders")
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_InterventionOrdersFilter value="Intervention Orders" checked="checked" />@item.V_VR_InterventionOrdersFilter
                                                }
                                                else
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_InterventionOrdersFilter value="Intervention Orders" />@item.V_VR_InterventionOrdersFilter
                                                }

                                                if (@item.V_VR_SelectFilter == "VOCAT")
                                                {
                                                    <input type="radio" style="margin-left:10px;" id="VRFilter" name=@item.V_VR_VOCATFilter value=@item.V_VR_VOCATFilter checked="checked" />@item.V_VR_VOCATFilter
