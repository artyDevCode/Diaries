                <tr>
                    <td class="auto-style12">
                        @if (Model != null)
                        {
                            int i = 0;
                            foreach (var item in Model)
                            {
                                if (i == 0)
                                {
                                    @Html.DropDownListFor(modelitem => item.V_VR_SelectName, new SelectList(item.V_VRSelect_Type, "Value", "Text", item.V_VR_SelectName), new { id = "V_VR_SelectName" });
                                }
                                i = i + 1;
                            }
                        }

                        <p style="font-family: Arial; font-size: 10pt; width: 720px; background-color: #46568c; color:white;">&nbsp;Active</p>

                        <div style="overflow:auto; height:115px; width:720px; font-family: arial; float: left; font-size: 9pt; background-color: whitesmoke;">

                            <table border="0" style="font-family: Arial; font-size: 9pt; width: 100%; background-color: white; float: left" cellspacing="0" cellpadding="0">
                                @foreach (var item in Model)
                                {
                                    if (item.V_VR_Deleted == false)
                                    {
                                        <tr style="background-color: threedlightshadow;">
                                            <td>
                                                @item.V_VR_Name
                                                <input type="button" id="@item.V_VR_id" class="VRMakeInactive" style="border:none;  float:right;" value="Make Inactive" />
                                            </td>
                                        </tr>
                                    }
                                }
                            </table>

                        </div>
                    </td>
                </tr>
            </table>

            <table border="1" style="font-family: Arial; float: right; font-size: 9pt; background-color: whitesmoke; width: auto; height: 100%; ">
                <tr style="background-color: #46568c; color: white; "><th align="center"><b>Inactive</b></th></tr>
                <tr>
                    <td class="auto-style12">
                        <div style="overflow:auto; height:115px; width:720px; font-family: arial; float: left; font-size: 9pt; background-color: whitesmoke;">

                            <table border="0" style="font-family: Arial; font-size: 9pt; width: 100%; background-color: white; float: left" cellspacing="0" cellpadding="0">
                                @foreach (var item in Model)
                                {
                                    if (item.V_VR_Deleted == true)
