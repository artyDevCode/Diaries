<table border="1" cellspacing="0" cellpadding="10">
    <tr>
        <td>
            <p style="font-family: Arial; font-size: 10pt; width: 100%;  background-color: #46568c; color:white;">&nbsp; List Types and List Attributes</p>


            <div id="DiaryListings" class="AjaxCall" style="overflow:auto; height:550px; width:740px; font-family: arial; float: left; font-size: 9pt; background-color: whitesmoke;">

                    <table border="0">

                        @foreach (var item in Model)
                            {
                                <tr class="Listings" id="@item.D_L_id" style="color: @item.D_L_Text_Colour; font-family: Arial; width: 740px; background-color: @item.D_L_Bckground_row_Colour;  float: left">
                                    <td class="ListMngId" hidden="hidden">
                                        @item.D_L_id
                                    </td>
                                    <td align="center" width="30px">
                                        @(item.D_L_Rules == true ? @Html.Raw("&#10004") : @Html.Raw("&#10008"))
                                    </td>
                                    <td class="ListMng" align="left" width="100px">
                                        @item.D_L_ShortName
                                    </td>
                                    <td class="ListMng" align="left" width="600px">
                                        @item.D_L_Lists
                                    </td>

                                     @if (ViewData["InSysAdminRole"] == "true")
                                        {
                                            <td align="center" width="70px">
                                                @if (item.Deleted == true)
                                                {<input type="button" id=@item.D_L_id class="ReinstateList" style="border: none; background-color: transparent; color: white; float: right;" value="Reinstate" />}
                                                else
                                                {<input type="button" id=@item.D_L_id class="DeleteList" style="border: none; background-color: transparent;  color:white; float: right;" value="Delete" />}
                                            </td>
                                        }
                                </tr>
                            }

                    </table>
                </div>

            <div id="ListAttrsSection">
                <div id="ListAttrsDetailTable"></div>
            </div>

